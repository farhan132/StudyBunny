# Production environment configuration
# Rename this file to .env.production and update with your values

# Backend API URL (replace with your Elastic Beanstalk URL)
REACT_APP_API_URL=https://your-app-name.elasticbeanstalk.com/api

# Or if using CloudFront/Custom Domain
# REACT_APP_API_URL=https://api.yourdomain.com/api

# Environment
REACT_APP_ENV=production

# Feature flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_ERROR_REPORTING=true

# Canvas integration
REACT_APP_CANVAS_ENABLED=true

